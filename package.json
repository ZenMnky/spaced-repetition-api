    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.21.16",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "postgrator-cli": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.20.0",
